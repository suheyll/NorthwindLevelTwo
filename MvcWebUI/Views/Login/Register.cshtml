@model UserListViewModel
@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}


<h2>Register</h2>


<title>Register</title>

<form asp-controller="Login" asp-action="Register" asp-route-userName="userName" asp-route-password="password"  method="post">
    <div class="form-group">
        <label asp-for="User.UserName">User Name</label>
        <input asp-for="User.UserName" class="form-control" name="userName" placeholder="userName"/>
    </div>
    <div class="form-group">
        <label asp-for="User.Password">Password</label>
        <input asp-for="User.Password" class="form-control" name="password" placeholder="password"/>
    </div>
    <input type="submit" value="Login" class="btn btn-success btn-group-sm"/>
</form>